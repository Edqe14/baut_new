{
    "name": "builderbaut",
    "version": "2.0.0",
    "description": "Official bot for the Buildergroop server",
    "scripts": {
        "clean": "rimraf dist",
        "build": "yarn clean && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist/src",
        "start": "node dist/src/client.js",
        "dev": "nodemon -r tsconfig-paths/register ./src/client.ts",
        "prisma:migrate": "prisma migrate",
        "prisma:push": "prisma db push",
        "prisma:generate": "prisma generate",
        "prisma:format": "prisma format",
        "prisma:genpush": "yarn prisma:generate && yarn prisma:push",
        "prettier": "prettier --write ."
    },
    "author": "Neesh774",
    "license": "MIT",
    "dependencies": {
        "@discordjs/rest": "^0.4.1",
        "@prisma/client": "^4.2.1",
        "chalk": "^4.1.0",
        "discord-api-types": "^0.33.0",
        "discord.js": "^14.3.0",
        "dotenv": "^10.0.0",
        "luxon": "^3.0.3",
        "moment": "^2.29.4",
        "moment-duration-format": "^2.3.2",
        "moment-timezone": "^0.5.33",
        "path": "^0.12.7",
        "require-all": "^3.0.0",
        "tscpaths": "^0.0.9"
    },
    "devDependencies": {
        "@types/luxon": "^3.0.0",
        "@types/moment-duration-format": "^2.2.3",
        "@types/node": "^14.14.10",
        "@types/require-all": "^3.0.3",
        "nodemon": "^2.0.6",
        "prettier": "^2.7.1",
        "prisma": "^4.2.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.8.0",
        "tsc-alias": "^1.7.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.1.2"
    }
}